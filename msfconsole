# update dabatase and start
service postgresql start
msfdb init
msfconsole
msf > db_status

# module infomation
info [module]

# module apply
use exploit/multi/handler

# see module parameters
show options

# set parameters
set [parameter] [value]

# set payload
set PAYLOAD [payload]

# set payload parameters
...

# set global parameters
setg

# set parameters to null
unset

# netcat-like quick connection
connect

# exploit
exploit

# save session within msf5 directory
save 

